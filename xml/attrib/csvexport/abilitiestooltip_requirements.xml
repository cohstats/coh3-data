<tuningtemplate csvfilename = "abilitiestooltip_requirements.csv" template="abilities" pathregex="^abilities" createnewlocstringsonimport="true" exportlocstringastext="true">
	<columns>
		<attribute columnname="requirement">
			<query name="default">
				<query name="ability_bag">
					<query name="requirements_target">
						<query name="required_unit_type" rowset="required_unit_type" usename="false">
							<value name="ui_name" />
						</query>
					</query>
				</query>
			</query>
		</attribute>
	</columns>
</tuningtemplate>