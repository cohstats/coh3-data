<tuningtemplate csvfilename = "ebpstooltip_environment_buildings.csv" template="ebps" pathregex="^ebps\\environment\\buildings" createnewlocstringsonimport="true" exportlocstringastext="true">
	<columns>
		<attribute columnname="screen_name">
			<query name="default">
				<query name="extensions">
					<query name="ui_ext">
						<value name="screen_name" />
					</query>
				</query>
			</query>
		</attribute>
		<attribute columnname="extra_text">
			<query name="default">
				<query name="extensions">
					<query name="ui_ext">
						<value name="extra_text" />
					</query>
				</query>
			</query>
		</attribute>
		<attribute columnname="brief_text">
			<query name="default">
				<query name="extensions">
					<query name="ui_ext">
						<value name="brief_text" />
					</query>
				</query>
			</query>
		</attribute>
				<attribute columnname="help_text">
			<query name="default">
				<query name="extensions">
					<query name="ui_ext">
						<value name="help_text" />
					</query>
				</query>
			</query>
		</attribute>
	</columns>
</tuningtemplate>