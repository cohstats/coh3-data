<tuningtemplate csvfilename = "upgradestooltip.csv" template="upgrade" pathregex="^upgrade" createnewlocstringsonimport="true" exportlocstringastext="true">
	<columns>
		<attribute columnname="screen_name">
			<query name="default">
				<query name="upgrade_bag">
					<query name="ui_info">
						<value name="screen_name" />
					</query>
				</query>
			</query>
		</attribute>
		<attribute columnname="extra_text">
			<query name="default">
				<query name="upgrade_bag">
					<query name="ui_info">
						<value name="extra_text" />
					</query>
				</query>
			</query>
		</attribute>
		<attribute columnname="brief_text">
			<query name="default">
				<query name="upgrade_bag">
					<query name="ui_info">
						<value name="brief_text" />
					</query>
				</query>
			</query>
		</attribute>
		<attribute columnname="help_text">
			<query name="default">
				<query name="upgrade_bag">
					<query name="ui_info">
						<value name="help_text" />
					</query>
				</query>
			</query>
		</attribute>
	</columns>
</tuningtemplate>