<template description="A loading screen that consists of 2 part - the background and a set of images on top of it" can_be_instantiated="True" virtual_category="" can_be_modded="False" export_for_client="True">
	<uniqueid name="pbgid" description="internal id for pbgs" serverpath="/UniqueId/NextElement/pbgid" />
	<group name="loading_screen_bag" description="" start_expanded="False">
		<file name="background" description="A path to the background image" file_ext="tga;png" file_root="ui\images\" file_helper_path="" file_default="" />
		<list name="screen_images" description="A list of screen images that go on top of the background" ordered="False" allow_duplicates="True" can_be_empty="True" start_expanded="False" flat="False">
			<file name="screen_image" description="" file_ext="tga;png" file_root="ui\images\" file_helper_path="" file_default="" />
		</list>
		<locstring name="title" description="" />
		<locstring name="subtitle" description="" />
		<locstring name="description" description="" />
		<string name="audio" description="" max_length="255" default="" />
	</group>
</template>